<mujocoinclude>

  <motor name="lft_wheel_motor" class="wheel" joint="lft_wheel_joint"/>
  <motor name="rgt_wheel_motor" class="wheel" joint="rgt_wheel_joint"/>

  <position name='lift' joint='slide_joint' class='slide_joint'/>
  <position name='head_yaw' joint='head_yaw_joint' class='head_yaw_joint'/>
  <position name='head_pitch' joint='head_pitch_joint' class='head_pitch_joint'/>

  <position name='lft_joint1' joint='lft_arm_joint1' class='joint1' kp="1000"/>
  <position name='lft_joint2' joint='lft_arm_joint2' class='joint2' kp="1000"/>
  <position name='lft_joint3' joint='lft_arm_joint3' class='joint3' kp="1000"/>
  <position name='lft_joint4' joint='lft_arm_joint4' class='joint4' kp='350'/>
  <position name='lft_joint5' joint='lft_arm_joint5' class='joint5' kp='350'/>
  <position name='lft_joint6' joint='lft_arm_joint6' class='joint6' kp='350'/>
  <position name='lft_gripper' tendon='lft_gripper_gear' ctrllimited="true" ctrlrange="0. 1." kp="3" user="1" forcelimited="true" forcerange="-1 1"/>

  <position name='rgt_joint1' joint='rgt_arm_joint1' class='joint1' kp="1000"/>
  <position name='rgt_joint2' joint='rgt_arm_joint2' class='joint2' kp="1000"/>
  <position name='rgt_joint3' joint='rgt_arm_joint3' class='joint3' kp="1000"/>
  <position name='rgt_joint4' joint='rgt_arm_joint4' class='joint4' kp='350'/>
  <position name='rgt_joint5' joint='rgt_arm_joint5' class='joint5' kp='350'/>
  <position name='rgt_joint6' joint='rgt_arm_joint6' class='joint6' kp='350'/>
  <position name='rgt_gripper' tendon='rgt_gripper_gear' ctrllimited="true" ctrlrange="0. 1." kp="3" user="1" forcelimited="true" forcerange="-1 1"/>

</mujocoinclude>
